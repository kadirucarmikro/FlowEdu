{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Flutter Web Chrome",
            "type": "dart",
            "request": "launch",
            "program": "lib/main.dart",
            "args": [
                "--web-port=5287"
            ],
            "deviceId": "chrome",
            "web": true
        },
        {
            "name": "Flutter Web Chrome (Hot Reload)",
            "type": "dart",
            "request": "launch",
            "program": "lib/main.dart",
            "args": [
                "--web-port=5287"
            ],
            "deviceId": "chrome",
            "web": true,
            "hotReload": true
        }
    ]
}
